    "# Plot the data with proper labeling\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.plot(monthly_sales[\"Date\"], monthly_sales[\"Sales\"], label=\"Monthly Retail Sales\", color='blue')\n",
