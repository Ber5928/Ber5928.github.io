    "# Reshape the data to have 'Date' and 'Sales' columns for easier plotting and modeling\n",
    "monthly_sales = df.melt(id_vars=[\"YEAR\"], var_name=\"Month\", value_name=\"Sales\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "8d59d92a-bfae-414d-91dc-45416eed989a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert 'Month' column to datetime format for easier sorting and plotting\n",
    "monthly_sales[\"Date\"] = pd.to_datetime(monthly_sales[\"YEAR\"].astype(str) + '-' + monthly_sales[\"Month\"], format=\"%Y-%b\")\n",
    "monthly_sales = monthly_sales.sort_values(by=\"Date\").reset_index(drop=True)"
   ]
  },
  {
